class Solution:
    def kthFactor(self, n: int, k: int) -> int:
        factors = []
        for i in range(1,n+1):
            if(n%i==0):
                factors.append(i)
        factors.sort()
        if(n==1):
            return 1
        elif(k>n):
            return -1
        elif(len(factors)<k):
            return -1
        else:
            return factors[k-1]        
            
        
